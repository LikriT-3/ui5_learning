{
	"_version": "1.12.0",
	"sap.app": {
		"id": "intheme.NewsProject",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZLOC_IVANOVA_NEWS_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"annotations": [
						"annotation"
					],
					"localUri": "localService/metadata.xml"
				}
			},
			"annotation": {
				"type": "ODataAnnotation",
				"uri": "localService/annotation.xml",
				"settings": {
					"localUri": "localService/annotation.xml"
				}
			}
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "intheme.NewsProject.view.App",
			"type": "XML",
			"async": true,
			"id": "flexibleColumnLayout"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.uxap": {}
			}
		},
		"config": {
			"fullWidth": true
		},
		"models": {
			"": {
				"dataSource": "mainService",
				"settings": {
					"serviceUrlParams": {
						"sap-client": "100"
					},
					"defaultBindingMode": "TwoWay",
					"useBatch": true,
					"defaultCountMode": "Inline"
				}
			},
			"state": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"isDraft": false,
					"icons": {
						"goHome": "sap-icon://home"
					},
					"currUser": "",
					"layout": "OneColumn"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "intheme.NewsProject.view",
				"controlId": "flexibleColumnLayout",
				"transition": "slide",
				"bypassed": {},
				"async": true
			},
			"routes": [
				{
					"pattern": ":layout:",
					"name": "master",
					"target": [
						"master"
					]
				},
				{
					"pattern": "detail/{news}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "nasa/{layout}",
					"name": "nasa",
					"target": [
						"nasa"
					]
				}
			],
			"targets": {
				"master": {
					"viewName": "Master",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"viewName": "Detail",
					"controlAggregation": "midColumnPages"
				},
				"nasa": {
					"viewName": "Nasa",
					"controlAggregation": "beginColumnPages"
				}
			}
		}
	}
}