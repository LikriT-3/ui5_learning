{
  "_version": "1.21.0",
  "sap.app": {
    "id": "intheme.zweather_app",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "ach": "ach",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.30.3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZWEATHER_SRV",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "annotations": [
            "annotation"
          ]
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "localService/annotation.xml",
        "settings": {
          "localUri": "localService/annotation.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_hcb",
      "sap_fiori_3_dark",
      "sap_belize"
    ],
    "fullWidth": true
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "intheme.zweather_app.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.71.22",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "intheme.zweather_app.i18n.i18n"
        }
      },
      "state": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "isDraft": false,
          "icons": {
            "goHome": "sap-icon://home"
          },
          "layout": "OneColumn",
          "editMode": false
        }
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "defaultBindingMode": "TwoWay",
          "useBatch": true,
          "defaultCountMode": "Inline"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "intheme.zweather_app.view",
        "controlAggregation": "beginColumnPages",
        "controlId": "layout"
      },
      "routes": [
        {
          "name": "WorklistRoute",
          "pattern": "",
          "target": [
            "WorklistTarget"
          ]
        },
        {
          "name": "TimeRoute",
          "pattern": "Time/:?query:",
          "target": [
            "TimeTarget",
            "WorklistTarget"
          ]
        }
      ],
      "targets": {
        "WorklistTarget": {
          "viewType": "XML",
          "viewName": "Worklist",
          "viewId": "Worklist",
          "viewLevel": "1",
          "controlAggregation": "beginColumnPages"
        },
        "TimeTarget": {
          "viewType": "XML",
          "viewId": "Time",
          "viewName": "Time",
          "viewLevel": "2",
          "controlAggregation": "midColumnPages"
        }
      }
    }
  }
}