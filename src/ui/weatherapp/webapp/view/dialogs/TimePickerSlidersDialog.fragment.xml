<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
    <Dialog
		id="selectTimeDialog"
		title="Select New Time">
        <timePickerSliders
			id="timePickerSliders"
			change="handleChange"
			height="400px"/>
        <buttons>
            <Button
				text="OK"
				press="handleOKPress"
				type="Emphasized"/>
            <Button
				text="Cancel"
				press="handleCancelPress"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>